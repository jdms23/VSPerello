    <COLUMN SOURCE="5" NAME="DESTINO_INDIRECTO" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="6" NAME="TRANSITARIO_DIRECTO" xsi:type="SQLVARYCHAR"/>
    <COLUMN SOURCE="7" NAME="TRANSITARIO_INDIRECTO" xsi:type="SQLVARYCHAR"/>
  </ROW>
</BCPFORMAT>
